{
  "name": "soulwax-dart-editor",
  "version": "0.5.0",
  "description": "Dart Editor web application for Dart compilation",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start server.js --name soulwax-dart-editor",
    "pm2:stop": "pm2 stop soulwax-dart-editor",
    "pm2:restart": "pm2 restart soulwax-dart-editor",
    "pm2:logs": "pm2 logs soulwax-dart-editor",
    "pm2:delete": "pm2 delete soulwax-dart-editor",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "monaco",
    "soulwax-dart-editor",
    "dart-editor",
    "dart",
    "editor"
  ],
  "author": "",
  "license": "GPL-3.0-only",
  "dependencies": {
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "express": "^4.21.2",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.7",
    "pm2": "^6.0.14"
  }
}
