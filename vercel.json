{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "rewrites": [
    {
      "source": "/compile",
      "destination": "/api"
    },
    {
      "source": "/health",
      "destination": "/api"
    },
    {
      "source": "/api/:path*",
      "destination": "/api"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 60,
      "includeFiles": "db/**"
    }
  }
}
