{
  "version": 2,
  "rewrites": [
    {
      "source": "/compile",
      "destination": "/api/server"
    },
    {
      "source": "/health",
      "destination": "/api/server"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/server"
    }
  ],
  "functions": {
    "api/server.js": {
      "maxDuration": 60,
      "includeFiles": "db/**"
    }
  }
}
